def print_non_repeated_elements(lst):
    element_freq = {}
    for element in lst:
        if element in element_freq:
            element_freq[element] += 1
        else:
            element_freq[element] = 1
    print("Non-Repeated Elements:")
    for element, frequency in element_freq.items():
        if frequency == 1:
            print(element)
my_list = [1, 2, 3, 4, 2, 1, 5, 6, 7, 8, 6]
print_non_repeated_elements(my_list)
